.year-header
  .year-header__left
    .year-header__left__logo
      = link_to "ASSET STATION", years_path, class: "asset-logo"
    .year-header__left__input
      = form_for([@year, @month], html: {class: "ym_input"}) do |f|
        ="#{@year.year}年"
        = f.number_field :month, class: "m-num"
        月
        = f.submit "次へ"
        
= render 'years/main_demoform'